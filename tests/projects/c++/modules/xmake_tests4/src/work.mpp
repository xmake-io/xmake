export module work;
import std;

export
template<typename T>
void do_work() {
    if constexpr (std::is_same_v<T, int>) {
        std::println("Hello world!");
    } else if constexpr (std::is_same_v<T, double>) {
        std::println("Hello test!");
    } else {
        std::println("Hello unknown type!");
    }
}
